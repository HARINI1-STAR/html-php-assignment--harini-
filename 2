<?php
echo "<table border='1' cellpadding='5'>";
echo "<tr><th> </th>";
for ($col = 1; $col <= 3; $col++) {
    echo "<th>$col</th>";
}
echo "</tr>";
for ($row = 1; $row <= 3; $row++) {
    echo "<tr>";
    echo "<th>$row</th>"; 
    for ($col = 1; $col <= 3; $col++) {
        $result = $row / $col;
        echo "<td>" . round($result, 2) . "</td>";
    }
    echo "</tr>";
}
echo "</table>";
?>

     1     2     3
1    1   0.5   0.33
2    2     1   0.67
3    3   1.5      1
